{
  "name": "ts-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/bin/serve",
    "watch": "ts-node ./bin/serve.ts",
    "dev": "nodemon --watch routes/**/* -e ts --exec ts-node ./bin/serve.ts",
    "build-ts": "tsc",
    "copy-static": "ts-node copyStatic.ts",
    "build": "yarn tsc && yarn copy-static"
  },
  "dependencies": {
    "@types/shelljs": "^0.8.11",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  }
}